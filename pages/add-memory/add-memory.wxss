.camera-content {
  position: relative;
  width: 150rpx;
  height: 150rpx;
  display: inline-block;
  margin: 20rpx 0 0 30rpx;
}

.camera-close-outter {
  position: absolute;
  top: -13rpx;
  right: -13rpx;
  width: 15px;
  height: 15px;
  background: #fff;
  font-size: 0;
  border-radius: 15px;
}

.camera-close {
  
}

.time-content-outter {
  padding: 0 30rpx;
}

.picker {
  color: #999;
}

.camera-picture {
  width: 150rpx;
  height: 150rpx;
}

.upload-content {
  position: fixed;
  bottom: 0;
  width: 100%;
  border-top: 1rpx solid #f3f3f3;
  padding: 20rpx 0;
  background: #fff;
}

.upload-content-item {
  width: 33%;
  display: inline-block;
  text-align: center;
  color: #eee;
}

.upload-button-outter {
  padding: 30rpx;
}